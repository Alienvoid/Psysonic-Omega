					ParseCondStateType="PARSE_DEFAULT">
					<Model Name="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP USER_6">
					<Model Name="EXCloakingField" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="PARALYZED USER_6">
					<Model Name="EXCloakingField" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_6">
					<Model Name="EXCloakingField" />
				</ModelConditionState>
				<!-- no mirage when damaged. -->
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model Name="" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_NoField"
					Flags="START_FRAME_FIRST">
					<Animation
					AnimationName="EXCloakingField"
					AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Field" then
						CurDrawableSetTransitionAnimState("TRANS_FromField")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_6 AFFECTED_BY_EMP"
					StateName="STATE_Field">
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Field" then
						CurDrawableSetTransitionAnimState("TRANS_FromField")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_6 PARALYZED"
					StateName="STATE_Field">
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Field" then
						CurDrawableSetTransitionAnimState("TRANS_FromField")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_6"
					StateName="STATE_Field">
					<Animation
						AnimationName="EXCloakingField"
						AnimationMode="ONCE" />
						<!-- <FXEvent Effect="FX_ALL_MirageFieldStart" FireWhenSkipped="true" /> -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_FromField" >
					<Animation
						AnimationName="EXCloakingField"
						AnimationMode="ONCE_BACKWARDS" />
						<!-- <FXEvent Effect="FX_ALL_MirageFieldStart" FireWhenSkipped="true" /> -->
				</AnimationState>
		</ScriptedModelDraw>
			
